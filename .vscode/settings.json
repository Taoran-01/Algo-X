{
	"workbench.colorTheme": "GitHub Dark",
	"workbench.startupEditor": "none",
	"files.autoSave": "afterDelay",
	"files.insertFinalNewline": true,
	"explorer.confirmDelete": false,
	"explorer.confirmDragAndDrop": false,
	"editor.fontFamily": "Consolas, 'Courier New', STFANGSONG, monospace",
	"editor.insertSpaces": false,
	"editor.rulers": [80],
	"explorer.autoReveal": false,
	"terminal.integrated.defaultProfile.windows": "Git Bash",
	"terminal.integrated.enableMultiLinePasteWarning": "never",
	"C_Cpp.autoAddFileAssociations": false,
	"git.enableSmartCommit": true,
	"git.confirmSync": false,
	"git.decorations.enabled": false,
	"scm.diffDecorationsGutterVisibility": "hover",
	"[plaintext]": {"editor.wordWrap": "on", "editor.rulers": [], "editor.inlineSuggest.enabled": false},
	"[markdown]": {"editor.wordWrap": "on", "editor.rulers": [], "editor.inlineSuggest.enabled": false},
	"markdown.preview.fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe WPC', 'Segoe UI', STFANGSONG, system-ui, 'Ubuntu', 'Droid Sans', sans-serif",
	"git.openRepositoryInParentFolders": "never",
}
